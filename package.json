{
  "name": "meseret",
  "version": "1.8.1",
  "description": "Opinionated, feature-packed professional node.js server framework.",
  "main": "lib",
  "typings": "lib",
  "scripts": {
    "del-cli": "del-cli",
    "run-s": "run-s",
    "run-p": "run-p",
    "prettier": "prettier",
    "tsc": "tsc",
    "precommit": "run-s prod && git add ./lib && git update-index --again",
    "prod": "run-s \"run-p format build\" format-lib",
    "lint": "prettier ./{*,src/**,tests/**}/**/*.{ts,js,json,md}",
    "format": "prettier --write ./{*,src/**,tests/**}/*.{ts,js,json,md}",
    "format-lib": "prettier --write ./lib/**/*.{ts,js,json,md}",
    "clean": "run-s \"del-cli lib *.tsbuildinfo\"",
    "build": "run-s clean tsc",
    "watch": "run-s clean \"tsc -w\"",
    "dev": "run-s watch",
    "test:build": "run-s prod && cd tests && del-cli dist *.tsbuildinfo && tsc",
    "test:start": "cd tests && node dist",
    "test": "run-s test:build test:start"
  },
  "keywords": [
    "meseret",
    "framework",
    "mongodb",
    "node.js",
    "typescript",
    "mongoose",
    "koa"
  ],
  "repository": "git@gitlab.com:kaleabmelkie/meseret",
  "author": "Kaleab Melkie <kaleabmelkie@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@types/gridfs-stream": "latest",
    "@types/koa": "latest",
    "@types/koa-compress": "latest",
    "@types/koa-json": "latest",
    "@types/koa-logger": "latest",
    "@types/koa-router": "latest",
    "@types/koa-send": "latest",
    "@types/koa-session": "latest",
    "@types/koa-static": "latest",
    "@types/koa-static-cache": "latest",
    "@types/mongoose": "latest",
    "@types/node": "latest",
    "@types/socket.io": "latest",
    "del-cli": "latest",
    "gridfs-stream": "latest",
    "husky": "latest",
    "koa": "latest",
    "koa-body": "latest",
    "koa-compress": "latest",
    "koa-convert": "latest",
    "koa-json": "latest",
    "koa-logger": "latest",
    "koa-router": "latest",
    "koa-send": "latest",
    "koa-session": "latest",
    "koa-static": "latest",
    "koa-static-cache": "latest",
    "mongoose": "latest",
    "nodemon": "latest",
    "npm-run-all": "latest",
    "prettier": "latest",
    "socket.io": "latest",
    "typescript": "latest"
  }
}
