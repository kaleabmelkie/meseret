{
  "name": "meseret",
  "version": "1.8.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Opinionated, feature-packed professional node.js server framework.",
  "keywords": [
    "meseret",
    "framework",
    "mongodb",
    "node.js",
    "typescript",
    "mongoose",
    "koa"
  ],
  "repository": "git@gitlab.com:kaleabmelkie/meseret",
  "author": "Kaleab S. Melkie <kaleabmelkie@gmail.com> (https://bit.ly/kaleab)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "run-p": "run-p",
    "run-s": "run-s",
    "prepublishOnly": "run-s prod",
    "clean": "del-cli dist *.tsbuildinfo",
    "build": "tsc",
    "lint": "prettier ./{*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,sass,json,md,html}",
    "format": "prettier --write ./{*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,sass,json,md,html}",
    "prod": "cross-env NODE_ENV=production run-s clean \"run-p format build test\"",
    "start": "run-s watch",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests && run-s demo-prod",
    "test-live": "jest --passWithNoTests --watch",
    "demo-prod": "cd demo && run-s prod",
    "demo-start": "cd demo && run-s start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prod && git update-index --again"
    }
  },
  "dependencies": {
    "@types/gridfs-stream": "latest",
    "@types/koa": "latest",
    "@types/koa-compress": "latest",
    "@types/koa-json": "latest",
    "@types/koa-logger": "latest",
    "@types/koa-router": "latest",
    "@types/koa-send": "latest",
    "@types/koa-session": "latest",
    "@types/koa-static": "latest",
    "@types/koa-static-cache": "latest",
    "@types/mongoose": "latest",
    "@types/node": "latest",
    "@types/socket.io": "latest",
    "cross-env": "latest",
    "del-cli": "latest",
    "gridfs-stream": "latest",
    "husky": "latest",
    "jest": "latest",
    "koa": "latest",
    "koa-body": "latest",
    "koa-compress": "latest",
    "koa-convert": "latest",
    "koa-json": "latest",
    "koa-logger": "latest",
    "koa-router": "latest",
    "koa-send": "latest",
    "koa-session": "latest",
    "koa-static": "latest",
    "koa-static-cache": "latest",
    "mongoose": "latest",
    "nodemon": "latest",
    "npm-run-all": "latest",
    "prettier": "latest",
    "socket.io": "latest",
    "typescript": "latest"
  }
}
